#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/cli/bin/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/cli/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/cli/bin/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/cli/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules/@storybook/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/@storybook+cli@7.6.4/node_modules:/Users/shanesteuteville/shane-dev/react-carousel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@storybook+cli@7.6.4/node_modules/@storybook/cli/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../@storybook+cli@7.6.4/node_modules/@storybook/cli/bin/index.js" "$@"
fi
